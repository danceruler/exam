
@{
}
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
    <title>@ViewBag.title</title>
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/zui/1.9.0/css/zui.min.css">
    <style>
        * {
            outline: none !important;
        }

        header {
            position: fixed;
            z-index: 99;
            top: 0;
            left: 0;
            width: 100%;
            height: 50px;
            line-height: 50px;
            font-size: 14px;
            color: #333;
            text-align: center;
            font-weight: bold;
            background-color: white;
            box-shadow: 0px 0px 18px 0px rgba(96, 96, 96, 0.15);
        }

        .backbox {
            position: absolute;
            padding: 0 10px;
            box-sizing: border-box;
        }

        .icon-chevron-left {
            font-size: 18px;
            line-height: 50px;
        }

        .list {
            padding-top: 20px;
        }

            .list h5 {
                margin: 0;
                font-weight: bold;
            }

        .item {
            overflow: hidden;
            line-height: 34px;
            padding-top: 10px;
            padding-bottom: 0px;
            border-bottom: 1px solid #eee;
        }

        .start {
            margin-top: 30px;
        }

        .announce {
            width: 100%;
            height: 100px;
            display: table-cell;
            vertical-align: middle;
            text-align: center;
            font-size: 20px
        }
    </style>
</head>


<body>

    <div class="container-fluid">
        <header>
            @*<div class="backbox" id="goback">
                    <i class="icon icon-chevron-left"></i>
                </div>*@
            @ViewBag.title
        </header>
        <div style="margin-top: 50px"></div>
        <div class="announce">
            <p style="margin-left: auto;
            margin-right: auto;">@ViewBag.Content</p>
        </div>
        <button id="@ViewBag.backUrl" class="btn btn-primary btn-block start" onclick="turnto(this)">@ViewBag.buttonContent</button>
    </div>


    <script src="//cdnjs.cloudflare.com/ajax/libs/zui/1.9.0/lib/jquery/jquery.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/zui/1.9.0/js/zui.min.js"></script>
    <script src="~/Scripts/js/const.js"></script>
    <!-- <script src="js/phone_rem.js"></script> -->
    <script>
        $(function () {

            $(goback).click(() => {
                window.history.back(-1);
            })

            //$(".start").on("click",()=>{

            //})
        })

        function turnto(obj) {
            window.location.href = obj.id;
        }
    </script>
</body>
